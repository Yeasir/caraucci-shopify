<header id="is-header" class="is-header header-style-2 fixed">
	<div class="header-container header-mainmenu ">

      
      			{{ settings.richtext_html }}

			{% comment %}
			<span class="htp-month">
				{{ settings.header_top_promo_month }}&mdash;</span><span class="htp-text">{{ settings.header_top_promo_text }} ></span>
			<span class="htp-use-code">code:</span>
			<strong>{{ settings.header_top_promo_code }}</strong>
			{% endcomment %}
      
      
      
		<div class="inner">
			<div class="header-inner clearfix container">
				<div class="logo pull-left">
					<div class="menu-action hidden-md hidden-lg pull-left" style="padding-top: 32px;">
					</div>
					<a href="/" class="logo-wrapper" itemprop="url">
						<img src="{{ settings.header_logo | img_url: 'master' }}" alt="{{ shop.name }}" itemprop="logo" class="img-responsive">
					</a>
				</div>
				<div class="button-action pull-right">
					<div class="menu-action hidden-md hidden-lg pull-right">
						<a href="#" data-uk-offcanvas="{target:'#is-off-canvas'}"><i class="icon-menu"></i></a>
					</div>
					<div class="shoppingcart pull-right icon-action">
						<a href="javascript:;" data-uk-offcanvas="{target:'#is_cart_canvas'}">
							<i class="icon-handbag icons"></i>
							<span class="count">{{ cart.item_count }}</span>
						</a>
					</div>
                  
                  
                        
					<div class="pull-right icon-action">
                        <div class="ssw-reward-tab-ico" id="ssw-reward-custom">
                          <svg xmlns="http://www.w3.org/2000/svg" width="16" height="15.2" viewBox="0 0 16 15.2"><path d="M14.4,3.2H12.64a2.842,2.842,0,0,0,.16-.8A2.357,2.357,0,0,0,10.4,0a2.291,2.291,0,0,0-2,1.04L8,1.6l-.4-.56A2.291,2.291,0,0,0,5.6,0,2.357,2.357,0,0,0,3.2,2.4a2.842,2.842,0,0,0,.16.8H1.6A1.6,1.6,0,0,0,0,4.8v8.8a1.6,1.6,0,0,0,1.6,1.6H14.4A1.6,1.6,0,0,0,16,13.6V4.8A1.6,1.6,0,0,0,14.4,3.2Zm-4-1.6a.756.756,0,0,1,.8.8.8.8,0,0,1-1.6,0A.756.756,0,0,1,10.4,1.6Zm-4.8,0a.756.756,0,0,1,.8.8.756.756,0,0,1-.8.8.756.756,0,0,1-.8-.8A.756.756,0,0,1,5.6,1.6Zm8.8,12H1.6V12H14.4Zm0-4H1.6v-4a.756.756,0,0,1,.8-.8H5.68L4.48,6.4a.846.846,0,0,0,.16,1.12.86.86,0,0,0,1.12-.16L8,4.32l2.24,3.04a.846.846,0,0,0,1.12.16.772.772,0,0,0,.16-1.12l-1.2-1.6H13.6a.8.8,0,0,1,.8.8Z" fill="#fff"></path></svg>
                        </div>
					</div>
                  
                  <script>
                    sswRun(function(){
                      ssw("#ssw-reward-custom").click(function(){ console.log('dsadsa');
                        ssw(".ssw-reward-tab.ssw-reward-tab-right").click();
                      });
                    });
                  </script>  
                  
                    {% capture the_snippet_auth %}{% render 'socialshopwave-widget-auth' %}{% endcapture %}
                    {% if the_snippet_auth contains 'Liquid error' %}
                     
					<div class="account-action pull-right icon-action">
						<a href="/account"><i class="icon-user"></i></a>
					</div>
                    
                    {% else %}
                      {% if shop.customer_accounts_enabled %}
                      <div id="ssw-topauth" class="ssw-topauth account-action pull-right icon-action">
                        {% if customer %}
                        <div class="ssw-tprofile ssw-dropdown account-action pull-right icon-action">
                          <a class="ssw-dropdown-toggle" data-toggle="ssw-dropdown" href="javascript:void(0);" style="font-size: 16px">
                           <i class="icon-user" style="font-size: 1.3em"></i>
                          </a>
                          <ul class="ssw-dropdown-menu" role="menu" aria-labelledby="dLabel">
                            <li id="customer_myorders_li"><a tabindex="-1" href="/account">{{ 'socialshopwave.my_orders' | t }}</a></li>
                            {% include 'ssw-widget-dropdown' %}
                            <li class="ssw-divider"></li>
                            <li><a id="customer_logout_link" tabindex="-1" href="/account/logout">{{ 'socialshopwave.logout' | t }}</a></li>
                          </ul>
                        </div>
                        {% else %}
                        <a id="customer_login_link" href="javascript:void(0);" data-toggle="ssw-modal" data-target="#login_modal" onclick="trackShopStats('login_popup_view', 'all')">
                          <i class="icon-user"></i>
                        </a>
                        {% endif %}
                      </div>
                      {% endif %}
                    {% endif %}
                    
					<div class="search-action pull-right icon-action">
						<a href="#"><i class="icon-magnifier"></i></a>
					</div>
				</div>
				<div class="hidden-sm header-menu header-menu-styte pull-right">
                    {% include 'module-megamenu-layout' %}
				</div>
			</div>

			<!-- Start Search Form -->
			<div class="is-search-form ">
                {% include 'module-search-header' %}
			</div>
			<!-- End Search Form -->
		</div>
	</div><!-- End .header-mainmenu -->
</header><!-- End .is-header -->
