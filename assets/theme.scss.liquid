                position: relative;
                border-bottom: 1px solid transparent;
                &:hover{
                    border-color: $title-color;
                    color: $title-color;
                }
            }
        }
	}
}



@media (min-width: 768px) {
    .is-megamenu.animate {

        [data-level="1"]{
            ul{
                .dropdown-menu{
                    top: 0;
                    left: 100%;
                }
                .menu-item{
                    .fa-angle-down{
                        position: absolute;
                        top: 50%;
                        right: 5px;
                        margin-top: -7px;
                        color: #999;
                    }
                }
            }
        }
        
        .mega{
            >.dropdown-menu{
                @include backface-visibility(hidden);          
                opacity: 0;
            }
            &.animating{
                >.dropdown-menu{
                    overflow: hidden;
                    display: block !important;
                }
            }
            &.open{
                >.dropdown-menu{
                    opacity: 1;
                }
                &.animating{
                    > .dropdown-menu{
                        opacity: 1;
                    }
                }
            }
        }

        //Effect Zoom
        &.zoom{
            .mega{
                > .dropdown-menu{
                    @include scale(0,0);
                    @include transform-origin-full(20%,20%); 
                }
                &.open{
                    > .dropdown-menu{
                        @include scale(1,1);
                    }
                }
            }
        }

        &.bottom-to-top{
            .megamenu {
                .dropdown-menu{
                    display: block;
                    margin: 20px 0 0 0;
                    opacity: 0;
                    visibility: hidden;
                    position: absolute;
                    left: 0;
                    z-index: 1;
                    .dropdown-menu{
                        top: 0;
                        left: 100%;
                        margin: 0 0 0 20px;     
                    } 
                }
                li{
                    &.open{
                        >.dropdown-menu{
                            opacity: 1;
                            visibility: visible;
                            margin: 0;
                        }
                    }
                }
            }
        }
        
        //Effect Elastic
        &.elastic{
            .mega{
                > .dropdown-menu {
                    @include scale(0,1);
                    @include transform-origin-full(10%,0);                   
                }
                &.open{
                    > .dropdown-menu {
                        @include scale(1,1);
                    }
                }
            }
            [data-level="1"]{
                &.mega > .dropdown-menu {
                    @include scale(1,0);
                }
                &.open{
                    > .dropdown-menu {
                        @include scale(1,1);
                    }
                }
            }
        }

        //Effect Slide
        &.slide{
            .mega{
                &.animating{
                    > .dropdown-menu {
                        overflow: hidden;
                    }
                }
                > .dropdown-menu > div {
                    @include transition(all 400ms);
                    @include backface-visibility(hidden);
                    margin-top: -100%;
                }
                &.open > .dropdown-menu > div {
                    margin-top: 0%;
                }
                .mega > .dropdown-menu {
                    min-width: 0;
                    > div{
                        min-width: 200px;
                        margin-top: 0;
                        margin-left: -500px;
                    }
                }
                .mega.open > .dropdown-menu > div {
                    margin-left: 0;
                }
            }
        }
    }
} 


