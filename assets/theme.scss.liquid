        li{
            border-color: rgba(#d4d4d4, 0.2);
            padding-bottom: 0;
            .image{
                margin-top: 0 !important;
                border: 0;
            }
        }
    }
    
    ul.nav{
        float: none;
        > li.open{
            a{
                background-color: transparent;
            }
        }
        > li{
            display: inline-block;
            float: none;
            text-align: left;
            margin-left: 24px;
            margin-right: 24px;
            &.aligned-center{
                >.dropdown-menu{
                    left:50%;
                    @include transform(translate(-50%));
                }
            }
            > a{
                color: $title-color;
                font-size: 12px;
                text-transform: uppercase;
                background: transparent;
                padding-left: 0;
                padding-right: 0;
                display: inline-flex;
                font-weight: $fw-demi;
                span{
                    position: relative;
                    font-family: $font-title;
                    &:after{
                        background-color: $title-color;
                        bottom: 0;
                        content: "";
                        height: 2px;
                        left: 0;
                        opacity: 0;
                        filter: alpha(opacity=0);
                        position: absolute;
                        width: 0;
                        -webkit-transition: width 0.6s ease;
                        -o-transition: width 0.6s ease;
                        transition: width 0.6s ease;
                    }
                }
            }
        }
    }

    // .level2{
    //     .dropdown-menu{
    //         left: 100%;
    //     }
    // }

    .megamenu{
        a,span{
            &:after{
                @include transition(all 0.4s);
            }
        }
        >li{
        	&:hover,
            &.current-menu-item,
            &.current-menu-parent,
            &.current-menu-ancestor{
                >a{
                    span{
                        &:after{
                            opacity: 1;
                            width: 100%;
                            background: $title-color;
                        }
                    }
                }
        	}

            .current-menu-item{
                >a{
                    >span{
                        color:$link-color;
                    }
                }
            }
            &.open{
                .woocommerce .product-col{
                    visibility: visible!important;
                }
            }
        }
        
        .mega-col-nav{
