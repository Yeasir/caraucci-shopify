$bg-color-banner-effect : rgba(#fff, 0.3);

div[class*="effect-"]{
	a{
		cursor: pointer;
	    position: relative;
	    overflow:hidden;
	    width: 100%;
        height: 100%;
	    display: block;
		&:before, &:after {
	        content:"";
	        display: block;
	        position: absolute;
	        width: 100%;
            height: 100%;
	        @include transition( all 0.3s ease-in-out 0s);
	    }    
	}
    img{
        max-width: 100%;
        height: auto;
    }
}

.effect-border { 
    a {    
        &:before{
            @include box-shadow (0 0 0 2px rgba(255, 255, 255, 0) inset);
            left: 0;
            top: 0;
            z-index: -1;
        }
        &:hover:before{
            @include box-shadow (0 0 0 30px rgba(255, 255, 255, 0.3) inset);
            z-index: 2;
        }
    }
}
.effect-full { 
    a {
        &:before{
            background: none repeat scroll 0 0 $bg-color-banner-effect;
            left: 0;
            @include opacity (0);
            top: 0;
            z-index: -1;    
        }
        &:hover:before {
            @include opacity (1);
            z-index: 2;
        }
    }        
}
.effect-bottom {
    a {
        &:before{
            background: none repeat scroll 0 0 $bg-color-banner-effect;
            left: 0;
            @include opacity (0);
            bottom: -100%;
            z-index: -1;
        }
        &:hover:before {
            bottom: 0;
            @include opacity (1);
            z-index: 2;
        }
    }    
}
.effect-top {
    a {
        &:before{
            background: none repeat scroll 0 0 $bg-color-banner-effect;
            left: 0;
            @include opacity (0);
            top: -100%;
            z-index: -1;
        }
        &:hover:before {
            top: 0;
            @include opacity (1);
            z-index: 2;
        }
    }    
}
.effect-right {
    a {
        &:before{
            background: none repeat scroll 0 0 $bg-color-banner-effect;
            top: 0;
            @include opacity (0);
            right: -100%;
            z-index: -1;
        }
        &:hover:before {
            right: 0;
            @include opacity (1);
            z-index: 2;
        }
    }    
}
.effect-left {
    a {
        &:before{
            background: none repeat scroll 0 0 $bg-color-banner-effect;
            top: 0;
            @include opacity (0);
            left: -100%;
            z-index: -1;
        }
        &:hover:before {
            left: 0;
            @include opacity (1);
            z-index: 2;
        }
    }    
}
.effect-rotate {
    a {
        &:before{
            background: none repeat scroll 0 0 $bg-color-banner-effect;
            top: 0;
            @include opacity (0);
            left: 0;
            z-index: -1;
            @include transform (rotate(720deg) scale(0));
        }
        &:hover:before {
            @include transform (rotate(0deg) scale(1));
            @include opacity (1);
            z-index: 2;
        }
    }    
}
.effect-rotate-cross {
    a {
        &:before{
            background: none repeat scroll 0 0 $bg-color-banner-effect;
            @include opacity (0);
            left: auto;
            right: 0;
            z-index: -1;
            @include transform (rotate(56.5deg) translateX(-180px));
            @include transform-origin (100% 0%);        
        }
        &:after {
            background: none repeat scroll 0 0 $bg-color-banner-effect;        
            @include opacity (0);
            top: auto;
            bottom: 0;
            z-index: -1;
            @include transform (rotate(56.5deg) translateX(180px));
            @include transform-origin (0% 100%);
        }
        &:hover{
            &:before {
                @include transform (rotate(56.5deg) translateX(1px));
                @include opacity (1);
                z-index: 2;
            }
            &:after {
                @include transform (rotate(56.5deg) translateX(-1px));
                @include opacity (1);
                z-index: 2;
            }
        }
    }    
}
.effect-in-to-out {
    a {
        &:before{
            background: none repeat scroll 0 0 $bg-color-banner-effect;
            top: 0;
            @include opacity (0);
            left: 50%;
            width: 0;
            height: 100%;
            z-index: -1;
        }
        &:hover:before {
            left: 0;
            width: 100%;
            @include opacity (1);
            z-index: 2;
        }
    }    
}
.effect-out-to-in {
    a {
        &:before{
            background: none repeat scroll 0 0 $bg-color-banner-effect;
            top: 0;
            @include opacity (0);
            left: -100%;
            z-index: -1;
        }
        &:after {
            background: none repeat scroll 0 0 $bg-color-banner-effect;
            top: 0;
            @include opacity (0);
            right: -100%;
            z-index: -1;
        }
        &:hover{
            &:before {
                left: -50%;
                @include opacity (1);
                z-index: 2;
            }
            &:after {
                right: -50%;
                @include opacity (1);
                z-index: 2;
            }
        }
    }    
