    border:none;
    min-height:0;
    float: right;
    font-size: 12px;
    margin-bottom: 0;
    .off-canvas-toggle{
        color: #fff;
        padding: 18px 20px 19px;
        min-width: 50px;
        text-align: center;
        display: none;
    }

    .fa-angle-down{
        display: inline-block;
        margin-left: 5px;
        padding-top: 5px;
    }

