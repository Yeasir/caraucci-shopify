    .navbar-collapse{
        padding: 0;
    }

    .navbar-nav.pull-right > li > .dropdown-menu, 
    .navbar-nav > li > .dropdown-menu.pull-right {
        left: 0;
    }

	.mega-group {
        min-width: 0!important;
        >a{
