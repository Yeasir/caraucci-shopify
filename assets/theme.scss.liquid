.effect-top-to-bottom {
    a {
        &:before{
            background: none repeat scroll 0 0 $bg-color-banner-effect;
            @include opacity (0);
            top: 0;
            width: 0;
            height: 0;
            left: 0;
            z-index: -1;
        }
        &:after {
            background: none repeat scroll 0 0 $bg-color-banner-effect;
            right: 0;
            bottom: 0;
            width: 0;
            height: 0;
            @include opacity (0);
            z-index: -1;
        }
        &:hover{
            &:before {
                width: 100%;
                height: 100%;
                @include opacity (1);
                z-index: 2;
            }
            &:after {
                width: 100%;
                height: 100%;
                @include opacity (1);
                z-index: 2;
            }
        }
    }    
}

