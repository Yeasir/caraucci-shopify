.home-banner-wrap {
  margin-left: -10px;
  margin-right: -10px;
}
.banner-item {
  position: relative;
  background: center center no-repeat transparent;
  background-size: cover;
  @media (max-width: 991px){
    background-position: left center;
  }
  @media (max-width: 480px){
    background-position: left center;
  }
  & > img {
    visibility: hidden;
    display: block;
    width: 100%;
    height: auto;
    @media (max-width: 480px){
      width: 115%;
      transform-style: preserve-3d;
    }
  }
  .banner-caption {
    right: 11%;
    transform: translate(7px, -50%);
    left: auto;
    top: 50%;
    position: absolute;
    line-height: 1;
    color: #000;
    text-shadow: none;
    margin-top: -5px;

    @media (max-width: 480px){
      transform: translate(70%, -50%);
      margin: 0;
      left: 6%;
      width: 280px;
      right: 0;
    }

    .banner-caption-inner {
      & > h3 {
        font-family: $font-helm;
        font-size: 72px;
        color: #7e3542;
        font-weight: 900;
        text-transform: uppercase;
        letter-spacing: 3px;
        margin-bottom: 0;
        @media (max-width: 991px){
          font-size: 40px;
          margin-top: 0;
        }
        @media (max-width: 480px){
          margin-top: 15px;
          font-size: 1.5em;
          letter-spacing: 0;
        }
      }
      & > h4 {
        font-family: $font-title;
        font-size: 20px;
        color: #000000;
        letter-spacing: .15em;
        margin: 5px;
        text-transform: uppercase;
        @media (max-width: 480px){
          font-size: .75em;
          margin-left: 0;
        }
      }

      .slide_discount_text {
        font-family: $font-helm;
        font-size: 35px;
        color: #7e3542;
        padding-top: 35px;
        @media (max-width: 480px){
          font-size: 1.2em !important;
          padding-top: 10px;
        }
      }
      .slide_under_discount_text {
        font-family: $font-helm;
        font-size: 1.1em;
        color: #000000;
        padding: 0 0 20px;
        @media (max-width: 480px){
          font-size: .75em;
          padding-bottom: 20px;
        }
      }
      .slide_coupon {
        letter-spacing: .2em;
        padding: 12px;
        width: 250px;
        text-align: center;
        white-space: nowrap;
        border: 1px dotted #000000;
        display: inline-block;
        @media (max-width: 767px){
          width: 180px;
        }
        @media (max-width: 480px){
          padding: 5px;
          width: 150px;
        }
        em {
          font-family: $font-helm;
          letter-spacing: 0;
          font-size: 1.2em;
          color: #000000;
          margin-bottom: 3px;
          display: block;
          @media (max-width: 480px){
            font-size: 1em;
          }
        }
        span {
          font-family: $font-title;
          font-size: 30px;
          color: #050408;
          font-weight: 400;
          display: block;
          @media (max-width: 480px){
            font-size: 1em;
          }
        }
      }
    }
  }
}
