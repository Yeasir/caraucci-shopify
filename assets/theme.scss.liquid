/* Hide radio buttons.*/
.swatch input { 
  display:none;
}
.swatch label {
  /* Rounded corners */
  -webkit-border-radius:2px;
  -moz-border-radius:2px;
  border-radius:2px;
  float: left;
  width:{{ width }}; 
  height:{{ height }};
  border:1px solid #ccc;
